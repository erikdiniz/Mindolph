<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<fx:root type="StackPane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane style="-fx-background-color: #D3D3D380;">
         <children>
            <HBox spacing="10.0">
               <children>
                  <ProgressIndicator fx:id="piProcessing" disable="true" prefHeight="20.0" prefWidth="20.0" />
                <Button fx:id="btnKeep" mnemonicParsing="false" text="Keep" />
                  <Button fx:id="btnRetry" mnemonicParsing="false" text="Retry" />
                  <Button fx:id="btnAdjust" mnemonicParsing="false" text="Adjust" />
                  <Button fx:id="btnAbandon" mnemonicParsing="false" text="Abandon" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
         </children>
      </AnchorPane>
   </children>
</fx:root>
